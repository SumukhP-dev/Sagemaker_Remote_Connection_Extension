{
  "name": "sagemaker-remote-connection",
  "displayName": "SageMaker Remote Connection",
  "description": "Connect Cursor/VS Code to AWS SageMaker Studio JupyterLab notebooks via Remote-SSH. One-click connection to SageMaker instances with automatic setup.",
  "version": "1.0.1",
  "publisher": "SumukhP-dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/SumukhP-dev/Sagemaker_Remote_Connection_Extension.git"
  },
  "homepage": "https://github.com/SumukhP-dev/Sagemaker_Remote_Connection_Extension#readme",
  "bugs": {
    "url": "https://github.com/SumukhP-dev/Sagemaker_Remote_Connection_Extension/issues"
  },
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other",
    "Remote",
    "Machine Learning"
  ],
  "keywords": [
    "aws",
    "sagemaker",
    "sagemaker studio",
    "jupyter",
    "jupyterlab",
    "notebook",
    "notebooks",
    "remote",
    "ssh",
    "remote-ssh",
    "cursor",
    "vscode",
    "machine learning",
    "ml",
    "data science",
    "python",
    "connect",
    "connection"
  ],
  "activationEvents": [
    "onCommand:sagemaker-remote.connect"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "sagemaker-remote.connect",
        "title": "Connect to SageMaker",
        "category": "SageMaker"
      },
      {
        "command": "sagemaker-remote.checkStatus",
        "title": "Check SageMaker Connection Status",
        "category": "SageMaker"
      },
      {
        "command": "sagemaker-remote.setup",
        "title": "Setup SageMaker Connection",
        "category": "SageMaker"
      },
      {
        "command": "sagemaker-remote.debugSSHConfig",
        "title": "Debug SSH Config",
        "category": "SageMaker"
      },
      {
        "command": "sagemaker-remote.startServer",
        "title": "Start Local Server",
        "category": "SageMaker"
      },
      {
        "command": "sagemaker-remote.diagnose",
        "title": "Diagnose Connection",
        "category": "SageMaker"
      },
      {
        "command": "sagemaker-remote.fixArnConversion",
        "title": "Fix ARN Conversion",
        "category": "SageMaker"
      },
      {
        "command": "sagemaker-remote.fixCodeWrapper",
        "title": "Fix Code Wrapper",
        "category": "SageMaker"
      },
      {
        "command": "sagemaker-remote.fixSshConfig",
        "title": "Fix SSH Config",
        "category": "SageMaker"
      },
      {
        "command": "sagemaker-remote.fixPowerShellDebugOutput",
        "title": "Fix PowerShell Debug Output",
        "category": "SageMaker"
      },
      {
        "command": "sagemaker-remote.applyAllFixes",
        "title": "Apply All Fixes",
        "category": "SageMaker"
      },
      {
        "command": "sagemaker-remote.quickStart",
        "title": "Quick Start: Connect to SageMaker",
        "category": "SageMaker"
      },
      {
        "command": "sagemaker-remote.cleanupRemoteServer",
        "title": "Clean Up Remote Server",
        "category": "SageMaker"
      },
      {
        "command": "sagemaker-remote.monitorRemoteServer",
        "title": "Monitor Remote Server (Real-time)",
        "category": "SageMaker"
      },
      {
        "command": "sagemaker-remote.quickStatusCheck",
        "title": "Quick Status Check",
        "category": "SageMaker"
      }
    ],
    "configuration": {
      "title": "SageMaker Remote Connection",
      "properties": {
        "sagemakerRemote.spaceArn": {
          "type": "string",
          "default": "",
          "description": "SageMaker Space ARN (e.g., arn:aws:sagemaker:us-east-1:123456789012:space/d-xxx/space-name)"
        },
        "sagemakerRemote.region": {
          "type": "string",
          "default": "us-east-1",
          "description": "AWS Region"
        },
        "sagemakerRemote.sshHostAlias": {
          "type": "string",
          "default": "sagemaker",
          "description": "SSH host alias in SSH config"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "vsce package --out releases/"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/vscode": "^1.80.0",
    "@vscode/vsce": "^2.32.0",
    "typescript": "^5.0.0"
  }
}
